class Solution {
public:
    vector<vector<int>> generate(int numRows) {
        vector<vector<int>>ans;
        vector<int>v;
        v.push_back(1);
        ans.push_back(v);
        if(numRows==1)return ans;
        v.push_back(1);
        ans.push_back(v);
        if(numRows==2)return ans;
        for(int i=2;i<numRows;i++){
            v.clear();
            v.push_back(1);
            int ind=1;
            for(int j=1;j<i;j++){
                v.push_back(ans[i-1][ind-1]+ans[i-1][ind]);
                ind++;
            }
            v.push_back(1);
            ans.push_back(v);
        }
        return ans;
    }
};