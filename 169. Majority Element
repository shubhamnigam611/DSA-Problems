class Solution {
public:
    int majorityElement(vector<int>& nums) {
        int count=0,cand;
        for(int i=0;i<nums.size();i++){
            if(cand==nums[i]){
                count++;
            }
            else if(count==0){
                cand=nums[i];
                count=1;
            }
            else
            count--;
        }
        return cand;
    }
};