class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        vector<int> ans;
        int p=1;
        for(int i=0;i<nums.size();i++)p*=nums[i];
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=0)
            ans.push_back(p/nums[i]);
            else{
                int pr=1;
                for(int j=0;j<nums.size();j++){
                    if(i!=j)pr*=nums[j];
                }
                ans.push_back(pr);
            }
        }
        return ans;
    }
};