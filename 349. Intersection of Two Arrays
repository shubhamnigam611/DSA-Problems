class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        set<int>s1,s2;
        vector<int>ans;
        for(int i:nums1)s1.insert(i);
        for(int i:nums2){
            if(s1.find(i)!=s1.end())
            s2.insert(i);
            }
        for(int i:s2){
            ans.push_back(i);
        }
        return ans;
    }
};