class Solution {
public:
    vector<int> plusOne(vector<int>& digits) {
        vector<int> ans;
        long long res=1,mul=1;
        if(digits[digits.size()-1]!=9)
        digits[digits.size()-1]+=1;
        else{
            int carry=1;
            for(int i=digits.size()-1;i>=0;i--){
                if(digits[i]==9&&carry){
                    digits[i]=0;
                    carry=1;
                }
                else if(carry){
                    digits[i]+=1;
                    carry=0;
                }
            }
            if(carry){
                digits.insert(digits.begin(),1);
            }
        }
        return digits;
    }
};