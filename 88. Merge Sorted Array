class Solution {
public:
    void merge(vector<int>& nums1, int m, vector<int>& nums2, int n) {
        int i=0,j=0;
        vector<int>res;
        while(i<m&&j<n){
            if(nums1[i]<nums2[j]){
                res.push_back(nums1[i]);
                i++;
            }
            else{
                res.push_back(nums2[j]);
                j++;
            }
        }
        if(i<m){
            for(;i<m;i++){
                res.push_back(nums1[i]);
            }
        }
        else if(j<n){
            for(;j<n;j++){
                res.push_back(nums2[j]);
            }
        }
        for(int i=0;i<m+n;i++){
            nums1[i]=res[i];
        }
    }
};